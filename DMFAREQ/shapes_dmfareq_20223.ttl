        sh:minCount 1 ;
        sh:maxCount 1 ; 
        sh:path [ sh:inversePath ont:R_90059_90082 ; ] ;
        sh:class ont:Form
    ] ;
.


kg:DmfAREQReferenceNbrShape a sh:PropertyShape;
