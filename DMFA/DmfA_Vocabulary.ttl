rdfs:domain 
            [ a owl:Class ;
                owl:unionOf (:WorkerRecord)
            ]
    ;
rdfs:isDefinedBy """
AAAA est égal à l'année de la déclaration."""@fr;
:length 10;
:type """Alphanumérique"""@fr;
.

