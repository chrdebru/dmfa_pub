        rr:objectMap [
            rml:reference "Categorie" ;
            rr:datatype xs:integer ;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate ont:Code ;
        rr:objectMap [
            rml:reference "Code" ;
            rr:datatype xs:integer ;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate rdfs:comment ;
        rr:objectMap [
            rml:reference "Remarquebijlage10" ;
            rr:language "fr" ;
        ] ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate ont:validFrom ;
